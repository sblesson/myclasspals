  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "port": 3000,
      "request": "attach",
      "type": "pwa-chrome",
      "webRoot": "${workspaceFolder}/src"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/frontend/src/reducers/group.js"
    }
  ]
}
